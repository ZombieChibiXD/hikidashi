{
  "name": "hikidashi",
  "version": "0.0.2",
  "private": false,
  "repository": "https://github.com/kaihodev/hikidashi",
  "author": "Kaihō Dev",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./safe": "./dist/safe.js",
    "./unsafe": "./dist/unsafe.js",
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:main": "tateru safe unsafe safe_bundle unsafe_bundle hikidashi_module hikidashi_bundle && ls dist",
    "build:types": "tsc -b src",
    "build:test": "tateru safe_tests_bundle unsafe_tests_bundle",
    "build:bench": "tateru benches_bundle",
    "build:mjs": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "dev": "tateru --watch --smap=\"inline\" & tsc -b -w --preserveWatchOutput src; wait",
    "test": "jest",
    "test:coverage": "jest --coverage=true",
    "bench": "sh run-all-benchmarks.sh",
    "prepack": "run-s \"build:?(main|types|mjs)\"",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.33",
    "@types/ramda": "^0.27.40",
    "@types/underscore": "^1.11.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "benchi": "^0.0.4",
    "benri": "^0.0.0-alpha-0",
    "eslint": "^7.21.0",
    "eslint-config-strict": "https://github.com/kaihodev/eslint-config-strict",
    "gen-esm-wrapper": "^1.1.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.27.1",
    "tateru": "^0.0.6",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "4.3.0-beta",
    "underscore": "^1.13.1"
  },
  "browserslist": [
    "> 0%"
  ]
}
